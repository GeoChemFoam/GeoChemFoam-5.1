FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.phase1 1
);

regions
(
    boxToCell
    {
        box (x1 y1 0) (x2 y2 1e-6);

        fieldValues
        (
            volScalarFieldValue alpha.phase1 0 
        );
    }
);



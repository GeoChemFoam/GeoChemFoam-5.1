FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Mflux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0; 

boundaryField
{
    left
    {
        type symmetryPlane;
    }

    walls
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}



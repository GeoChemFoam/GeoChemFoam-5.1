FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue eps 1 
);

regions
(
    boxToCell
    {
        box (0 2e-3 0) (5e-3 4e-3 0.1e-3);
        fieldValues
        (
            volScalarFieldValue eps 0.001 
        );
    }
);



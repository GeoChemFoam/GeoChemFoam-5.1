FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
    boxToCell
    {
        box (lx1 ly1 lz1) (lx2 ly2 lz2);

        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }

);


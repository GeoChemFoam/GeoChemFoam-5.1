FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      A;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    inlet
    {
	type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
	type            zeroGradient;
    }

    fixedWalls
    {
        type           zeroGradient;
    }

    frontandback
    {
        type            empty;
    }

}


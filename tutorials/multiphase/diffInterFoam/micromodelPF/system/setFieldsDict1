FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1 
);

regions
(
    boxToCell
    {
        box (0 0 0) (xmax ymax zmax);
        fieldValues
        (
            volScalarFieldValue alpha.water 0 
        );
    }
);


